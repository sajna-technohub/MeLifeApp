<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="style_answer">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/darkblue</item>
        <item name="android:textStyle">bold</item>
        <item name="android:typeface">serif</item>
    </style>
    <style name="style_answer_button">
        <item name="android:textSize">13dp</item>
        <item name="android:gravity"> center</item>
        <item name="android:paddingLeft"> @dimen/margin_20</item>
        <item name="android:layout_marginLeft"> @dimen/dimen_10</item>
        <item name="android:drawableLeft"> @drawable/ic_radio_button_unchecked_black_24dp</item>
        <item name="android:layout_marginRight"> @dimen/dimen_10</item>
        <item name="android:typeface">serif</item>
        <item name="android:textColor">@color/questionblack</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">@dimen/margin_20</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@drawable/exam_btn_background</item>
    </style>
    <style name="style_answer_image">
        <item name="android:textSize">13dp</item>
        <item name="android:gravity"> center</item>
        <item name="android:typeface">serif</item>
        <item name="android:textColor">@color/whiteLogocolor</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">40dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:textAllCaps">false</item>

    </style>

    <style name="style_skill_profile">
        <item name="android:textSize">@dimen/dimen_15</item>
        <item name="android:fontFamily">@font/roboto_light</item>
        <item name="android:textColor">@color/questionblack</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/dimen_10</item>
        <item name="android:layout_marginRight">@dimen/dimen_10</item>
    </style>



    <style name="style_skill_report_description">
    <item name="android:textSize">@dimen/dimen_15</item>
    <item name="android:lineSpacingExtra">@dimen/dimen_15</item>
    <item name="android:fontFamily">@font/barlowsemicondensedlight</item>
    <item name="android:textColor">@color/questionblack</item>
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:paddingLeft">@dimen/margin_20</item>
    <item name="android:paddingRight">@dimen/margin_20</item>
    <item name="android:layout_marginTop">@dimen/dimen_10</item>
    </style>

    <style name="style_skill_report_title">
        <item name="android:textSize">@dimen/margin_20</item>
        <item name="android:fontFamily">@font/barlowsemicondensedmedium</item>
        <item name="android:textColor">@color/hardwhite</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_60</item>
        <item name="android:clickable">true</item>
        <item name="android:elevation">4dp</item>
        <item name="android:gravity">left</item>
        <item name="android:padding">@dimen/dimen_15</item>
    </style>
    <style name="style_stream_report_title">
        <item name="android:textSize">@dimen/margin_20</item>
        <item name="android:fontFamily">@font/barlowsemicondensedmedium</item>
        <item name="android:textColor">@color/hardwhite</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/streamdrawable</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_60</item>
        <item name="android:clickable">true</item>
        <item name="android:elevation">4dp</item>
        <item name="android:gravity">left</item>
        <item name="android:padding">@dimen/dimen_15</item>
    </style>
    <style name="style_ai_report_title">
        <item name="android:textSize">@dimen/margin_20</item>
        <item name="android:fontFamily">@font/barlowsemicondensedmedium</item>
        <item name="android:textColor">@color/hardwhite</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/aidrawable</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_60</item>
        <item name="android:clickable">true</item>
        <item name="android:elevation">4dp</item>
        <item name="android:gravity">left</item>
        <item name="android:padding">@dimen/dimen_15</item>
    </style>
    <style name="style_humanity_report_title">
        <item name="android:textSize">@dimen/margin_20</item>
        <item name="android:fontFamily">@font/barlowsemicondensedmedium</item>
        <item name="android:textColor">@color/hardwhite</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/humanitydrawable</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_60</item>
        <item name="android:clickable">true</item>
        <item name="android:elevation">4dp</item>
        <item name="android:gravity">left</item>
        <item name="android:padding">@dimen/dimen_15</item>
    </style>
    <style name="style_skill_table_title">
        <item name="android:textSize">@dimen/dimen_15</item>
        <item name="android:fontFamily">@font/barlowsemicondensedmedium</item>
        <item name="android:textColor">@color/btn_blue</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_60</item>
        <item name="android:gravity">left</item>

    </style>
    <style name="style_com_table_title">
        <item name="android:textSize">@dimen/dimen_15</item>
        <item name="android:fontFamily">@font/barlowsemicondensedmedium</item>
        <item name="android:textColor">@color/hardwhite</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/border</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_weight">0.5</item>
        <item name="android:layout_height">@dimen/dimen_50</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="style_hum_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">0.25</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:fontFamily">@font/barlowsemicondensedlight</item>
        <item name="android:textColor">@color/hardwhite</item>
    </style>


    <style name="style_skill_table_item">
        <item name="android:layout_width">@dimen/dimen_100</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">0.25</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@color/transparent_blue</item>
        <item name="android:gravity">center</item>
        <item name="android:fontFamily">@font/barlowsemicondensedlight</item>
        <item name="android:textColor">@color/hardwhite</item>
    </style>
    <style name="style_hum_table_item">
        <item name="android:layout_width">@dimen/margin_200</item>
        <item name="android:layout_height">@dimen/dimen_50</item>
        <item name="android:layout_weight">0.25</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:fontFamily">@font/barlowsemicondensedlight</item>
        <item name="android:textColor">@color/hardwhite</item>
    </style>

    <style name="style_skill_report_title_blue">
        <item name="android:textSize">@dimen/margin_20</item>
        <item name="android:fontFamily">@font/barlowsemicondensedmedium</item>
        <item name="android:textColor">@color/hardwhite</item>
        <item name="android:background">@drawable/reportdrawable</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginTop">@dimen/margin_20</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:elevation">4dp</item>
        <item name="android:gravity">left</item>
        <item name="android:padding">@dimen/dimen_15</item>
    </style>
    <style name="style_commerce_report_title_red">
        <item name="android:textSize">@dimen/dimen_15</item>
        <item name="android:fontFamily">@font/barlowsemicondensedmedium</item>
        <item name="android:textColor">@color/hardwhite</item>
        <item name="android:background">@drawable/commercedrawable</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginTop">@dimen/margin_20</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:elevation">4dp</item>
        <item name="android:gravity">left</item>
        <item name="android:padding">@dimen/dimen_15</item>
    </style>
    <declare-styleable name="CircleProgress">
        <attr name="circle_progress" format="integer"/>
        <attr name="circle_max" format="integer"/>
        <attr name="circle_unfinished_color" format="color"/>
        <attr name="circle_finished_color" format="color"/>
        <attr name="circle_text_size" format="dimension"/>
        <attr name="circle_text_color" format="color"/>
        <attr name="circle_prefix_text" format="string"/>
        <attr name="circle_suffix_text" format="string"/>
    </declare-styleable>
</resources>
